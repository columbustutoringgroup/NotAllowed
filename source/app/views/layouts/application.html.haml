!!! 5
%html
  %head
    %title Source
    = stylesheet_link_tag 'application', { media: 'all', 'data-turbolinks-track' => true }
    = javascript_include_tag 'application', { 'data-turbolinks-track' => true }
    = csrf_meta_tags
  %body

    = render 'partials/nav'

    .container.primary-body
      - if flash.any?
        %div.errors
          - flash.each do |_, message|
            %p= message
      = yield

